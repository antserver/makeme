{
    control: {
        "?listen": '127.0.0.1:4000'
    },
    debug: {
        services: {
            "?gzip": false,
            "?html": false,
            "?js": {
                "?minify": false,
                "?usemin": true,
                "?usemap": true
            }
        }
    },
    release: {
        services: {
            "css": {
                "?minify": true,
                "?usemin": true,
                "?usemap": false
            },
            "?gzip": true,
            "js": {
                "?minify": true,
                "?usemin": true,
                "?usemap": false
            },
            "?reload": false
        }
    }
}
