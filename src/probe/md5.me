/*
    md5.me - MD5 probe
 */

Me.extension({
    md5: {
        description: 'Message Digest Tool',
        path: function (extension) {
            let md5 = probe(extension.withpath || 'md5', {'continue': true, fullpath: true})
            if (!md5) {
                md5 = probe('md5sum', {fullpath: true})
            }
            return md5
        },
    },
})

