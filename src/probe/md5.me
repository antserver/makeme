/*
    md5.me - MD5 probe
 */

Me.extension({
    md5: {
        description: 'Message Digest Tool',
        path: function (extension) {
            if (me.options.gen) {
                return "md5"
            }
            let path: Path = extension.withpath || 
                probe('md5', {'continue': true, fullpath: true}) ||
                probe('md5sum', {fullpath: true})
            return path
        },
    },
})

