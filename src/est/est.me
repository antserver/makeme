/*
    est.me -- MakeMe file for Embedded Secure Transport
 */

Me.load({
    blend: [ 'osdep' ],

    targets: {
        libest: {
            type: 'lib',
            static: true,
            headers: [ '*.h' ],
            depends: [ 'osdep' ],
            sources: [ 'estLib.c' ],
            '-compiler': [
                '-Wall',
                '-Wshorten-64-to-32',
                '-W3',
            ],
            ifdef: [ 'est' ],
        }
	}
})
